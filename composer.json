{
  "name": "cedricziel/typo3-cegenerator",
  "description": "Generator and Maintenance tool for TYPO3 extensions and Content Elements",
  "require": {
    "doctrine/common": "^2.6",
    "league/flysystem": "^1.0",
    "league/flysystem-memory": "^1.0",
    "symfony/config": "^3.0",
    "symfony/console": "^3.0",
    "symfony/dependency-injection": "^3.0",
    "symfony/event-dispatcher": "^3.0",
    "symfony/filesystem": "^3.1",
    "symfony/property-access": "^3.1",
    "symfony/serializer": "^3.1",
    "symfony/yaml": "^3.0",
    "twig/twig": "^1.24"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "CedricZiel\\T3CETool\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CedricZiel\\T3CETool\\Test\\": "test/"
    }
  },
  "authors": [
    {
      "name": "Cedric Ziel",
      "email": "cedric@cedric-ziel.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^5.4"
  },
  "config": {
    "sort-packages": true
  }
}
